<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_METHODS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="JAVADOC_STUB_ON_ENTER" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="PHP.parameter.hints.old" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_FUNCTION_BODY" value="true" />
    <option name="COLLAPSE_METHOD_BODY" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="insertQuotesForAttributeValue" value="false" />
  </component>
</application>